    <!-- 아래 내용을 추가해야 ShareServlet 사용 가능 -->
    <servlet>
        <servlet-name>share-servlet</servlet-name>
        <servlet-class>jsp.servlet.ShareServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>share-servlet</servlet-name>
        <url-pattern>/jsp.servlet.ShareServlet</url-pattern>
    </servlet-mapping>
